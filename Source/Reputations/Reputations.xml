        </KeyValues>
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.CobaltAssembly" relativePoint="BOTTOMLEFT" y="-2" />
        </Anchors>
      </Frame>
      <Frame parentKey="Wrathion" inherits="DFH_FriendItemFrame">
        <KeyValues>
          <KeyValue key="factionId" value="2517" type="number" />
        </KeyValues>
        <Anchors>
          <Anchor point="TOPLEFT" relativeKey="$parent.Sabellian" relativePoint="BOTTOMLEFT" y="-2" />
        </Anchors>
      </Frame>
    </Frames>
  </Frame>
</Ui>